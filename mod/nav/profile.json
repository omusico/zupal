{
    "name": "Navigation",
    "description" : "A module for creating and maintaining navigation UI",
    "deps": [],
    "version": [1,0,0],
    "package": "core",

    "handlers": {
        "route": {
            "any": ["Nav_Handler_Route"]
        },
        "menu": {
            "any": ["Nav_Handler_Menu"]
        }
    },

    "resources":[
        {
            "namespace": "Nav",
            "types": [
                {
                    "name": "model",
                    "dir": "model/",
                    "class": "Model"
                },
                {
                    "name": "view",
                    "dir": "view/",
                    "class": "View"
                },
                {
                    "name": "handler",
                    "dir": "handler/",
                    "class": "Handler"
                }
            ]
        }
    ]
}
