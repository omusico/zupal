package game.synerg.buttons{	import flash.display.SimpleButton;	import flash.events.Event;	import flash.ui.Mouse;	import flash.events.MouseEvent;	import Synerg;	import game.Game;	public class Ranch extends SimpleButton {				public function Ranch(g: Game){			_game = g;			addEventListener(MouseEvent.MOUSE_DOWN, add_ranch);		}		private function add_ranch(e:Event) {			_game.add_ranch();		}				private var _game:Game;	}}